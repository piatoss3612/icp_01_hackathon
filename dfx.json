{
    "canisters": {
        "ledger": {
            "type": "custom",
            "candid": "src/ledger/ledger.did",
            "wasm": "src/ledger/ledger.wasm"
        },
        "nft": {
            "type": "custom",
            "main": "src/nft/index.ts",
            "candid": "src/nft/nft.did",
            "build": "npx azle nft",
            "wasm": ".azle/nft/nft.wasm",
            "gzip": true
        },
        "backend": {
            "type": "custom",
            "main": "src/backend/index.ts",
            "candid": "src/backend/backend.did",
            "build": "npx azle backend",
            "wasm": ".azle/backend/backend.wasm",
            "gzip": true
        }
    },
    "defaults": {
        "build": {
            "args": "",
            "packtool": ""
        }
    },
    "output_env_file": ".env",
    "version": 1
}