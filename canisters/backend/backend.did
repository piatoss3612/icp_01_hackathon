    getUserExhibitions: (text) -> (vec record {id:text; owner:text; name:text; description:text; artworks:vec record {owner:text; name:text; description:text; image:vec nat8; comments:vec record {content:text; owner:text; adapted:bool; exhibition:text}; price:nat; onSale:bool; exhibition:text}; onExhibition:bool; ticketImage:vec nat8; ticketPrice:nat}) query;
